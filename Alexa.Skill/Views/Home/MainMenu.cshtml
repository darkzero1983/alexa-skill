@model MainMenuViewModel

<li>@Html.ActionLink("Skill Übersicht", "Index", "Home")</li>

<li class="dropdown">
	<a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Intents <span class="caret"></span></a>
	<ul class="dropdown-menu">
		<li>@Html.ActionLink("Übersicht", "Intents", "Skill", new { id = Model.SkillId }, null)</li>
		<li role="separator" class="divider"></li>
		@foreach (KeyValuePair<int, string> intent in Model.Intents)
		{
			<li>@Html.ActionLink(intent.Value, "Detail", "Intent", new { id = intent.Key }, null)</li>
		}
	</ul>
</li>

<li>@Html.ActionLink("Welcome Messages", "WelcomeMessages", "Skill", new { id = Model.SkillId }, null)</li>
<li>@Html.ActionLink("Statistik", "Index", "Statistic", new { id = Model.SkillId }, null)</li>
<li>@Html.ActionLink("Konfig", "Index", "SkillConfig", new { id = Model.SkillId }, null)</li>